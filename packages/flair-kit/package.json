{
  "name": "flair-kit",
  "version": "0.0.3",
  "scripts": {
    "dev": "concurrently pnpm:dev:*",
    "dev:esm": "pnpm build:esm -- --watch -C jsc.transform.react.development=true -C jsc.transform.react.runtime=automatic",
    "dev:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types --watch",
    "build": "concurrently pnpm:build:*",
    "build:esm": "swc ./src -d dist/esm --config-file .swcrc",
    "build:cjs": "swc ./src -d dist/cjs --config-file .swcrc -C module.type=commonjs",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "generate-colors": "ts-node ./utils/generateColors --skip-project",
    "prepublish": "pnpm build"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@k-vyn/coloralgorithm": "^1.0.0",
    "@swc/cli": "^0.1.40",
    "@swc/core": "^1.2.58",
    "@types/next": "^9.0.0",
    "@types/node": "^13.7.1",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.4",
    "classnames": "^2.3.1",
    "concurrently": "5.3.0",
    "cross-env": "7.0.3",
    "goober": "^2.0.37",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.0"
  },
  "peerDependencies": {
    "classnames": "^2.3.1",
    "goober": "^2.0.37",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@reach/tooltip": "^0.15.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackyef/flair.git"
  },
  "author": "Jacky Efendi",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
